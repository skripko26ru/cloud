		meta(charset='UTF-8')
		meta(name='viewport' content='width=device-width, initial-scale=1.0')
		meta(http-equiv='X-UA-Compatible' content='ie=edge')
		link(rel="shortcut icon" href="favicon.png" type="image/x-icon")

		block head		
			+head('Digital Cloud')

	body.screen-0
