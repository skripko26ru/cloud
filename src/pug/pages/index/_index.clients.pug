	var clientList = [
		{ name: 'ВТБ', logo: '/../assets/img/_icon-vtb.png'},
		{ name: 'УралСиб', logo: '/../assets/img/_icon-ural.png'},
		{ name: 'ПромСвязьБанк', logo: '/../assets/img/_icon-psb.png'},
